{
  "name": "open-agent-system",
  "version": "0.1.0",
  "private": true,
  "description": "Multi-agent LLM-agnostic orchestration platform with 50 extensibility hooks",
  "keywords": [
    "llm",
    "multi-agent",
    "orchestration",
    "openrouter",
    "mcp",
    "ai",
    "typescript"
  ],
  "homepage": "https://github.com/FutureTranz-Inc/open-agent-system",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FutureTranz-Inc/open-agent-system.git"
  },
  "license": "MIT",
  "author": "FutureTranz Inc.",
  "type": "module",
  "workspaces": [
    "packages/*",
    "packages/adapters/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:core": "pnpm --filter @open-agent/core build",
    "build:cli": "pnpm --filter @open-agent/cli build",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "turbo run typecheck",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "typedoc": "^0.26.0",
    "typedoc-plugin-markdown": "^4.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "@types/node": "^22.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.3.0",
    "turbo": "^2.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
